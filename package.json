{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "React SSR",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node build/bundle.js",
    "build": "npm-run-all --parallel build:*",
    "build:prod-client": "webpack --mode production --config ./config/webpack/prod.client.config.js ",
    "build:prod-server": "webpack --mode production --config ./config/webpack/prod.server.config.js",
    "dev": "npm run -dev build && nodemon --watch build --exec \"node build/bundle.js\" ",
    "dev-build": "npm-run-all --parallel dev:*",
    "dev:build-server": "webpack --mode development --config ./config/webpack/dev.server.config.js --watch",
    "dev:build-client": "webpack --mode development --config ./config/webpack/dev.client.config.js --watch",
    "lint": "eslint \"./src/**/*.js\"",
    "test": "jest --colors --config ./config/jest-config.js",
    "test:watch": "jest --watch --verbose --colors --config ./config/jest-config.js",
    "test:coverage": "jest --colors --verbose --coverage --config ./config/jest-config.js",
    "clear-build": "rm -rfv build/* rm -rfv public/*"
  },
  "author": "",
  "license": "ISC",
